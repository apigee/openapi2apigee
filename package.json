    "openapi-utils-schema-from-api": "0.0.4",
    "policify": "0.0.12",
    "rimraf": "^2.5.4",
    "should": "^11.1.1",
    "standard": "^17.0.0",
    "uglify-js": "^2.7.4",
    "xml2js": "^0.4.17",
    "z-schema": "^5.0.5"
  },
  "standard": {
